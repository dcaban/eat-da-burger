<div class="container z-depth-2 hoverable" id="content_container">
  <div class="row">
    <div class="col s6 center-align">
      <h4>Burgers that are not devoured!</h4>
      <hr>

      <ul>
        {{#each burgers}}
          {{#unless devoured}}
            {{> burgers/burger-block devour=true}}
          {{/unless}}
        {{/each}}
      </ul>
      </div>
      
      
<div class="col s6 center-align">
      <h4>Burgers that have been devoured!</h4>
      <hr>

      <ul>
        {{#each burgers}}
          {{#if devoured}}
            {{> burgers/burger-block devour=false}}
          {{/if}}
        {{/each}}
      </ul>
      </div>
      </div>
      <div class="row">
  <div class="col s8 offset-s2">
      <h2>Add a Burger</h2>
      <form class="create-form">

        <div class="form-group">
          <label for="ca">Burger Name:</label>
          <input type="text" id="ca" name="name">
        </div>


        <button class="waves-effect waves-light btn" type="submit">Add burger</button>
      </form>
      </div>
   </div>
</div>
  

